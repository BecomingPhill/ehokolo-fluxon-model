      "source": [
        "## Initial Conditions and Simulation Loop for Complex Eholokon\n",
        "Initialize with a Gaussian pulse for \(\\phi_r\) and zero for \(\\phi_i\) to find a non-spinning charged soliton. \n",
        "Alternatively, initialize with a vortex ansatz (e.g., \(\\phi = f(r)e^{in\\theta}\)) to seek spinning solutions."
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "main_loop_code_efm_spin_charge_v2"
      },
      "source": [
        "# --- Initialize Fields on CPU for Complex Eholokon Simulation --- \n",
        "print(\"Initializing COMPLEX fields for Charge/Spin Analogue simulation...\")\n",
        "\n",
        "# Use parameters from config_efm_sc\n",
        "N_efm = config_efm_sc['N']\n",
        "L_sim_efm = config_efm_sc['L_sim']\n",
        "dx_efm = config_efm_sc['dx_sim']\n",
        "A0_efm = config_efm_sc['A0_initial_pulse']\n",
        "w0_efm = config_efm_sc['w0_initial_pulse']\n",
        "winding_n = config_efm_sc['initial_winding_number']\n",
        "\n",
        "coords_efm = torch.arange(-L_sim_efm/2 + dx_efm/2, L_sim_efm/2 + dx_efm/2, dx_efm)\n",
        "X_grid_efm, Y_grid_efm, Z_grid_efm = torch.meshgrid(coords_efm, coords_efm, coords_efm, indexing='ij')\n",
        "\n",
        "phi_r_initial_cpu = torch.zeros((N_efm, N_efm, N_efm), dtype=torch.float32)\n",
        "phi_i_initial_cpu = torch.zeros((N_efm, N_efm, N_efm), dtype=torch.float32)\n",
        "\n",
        "if winding_n == 0: # For non-spinning charged soliton attempt\n",
        "    print(\"Initializing for non-spinning (charge focus) soliton.\")\n",
        "    r_sq_efm = X_grid_efm**2 + Y_grid_efm**2 + Z_grid_efm**2\n",
        "    phi_r_initial_cpu = (A0_efm * torch.exp(-r_sq_efm / (w0_efm**2))).to(dtype=torch.float32)\n",
        "    # phi_i_initial_cpu remains zero\n",
        "    del r_sq_efm\n",
        "else: # For vortex / spin attempt\n",
        "    print(f\"Initializing with winding number: {winding_n} for spin focus.\")\n",
        "    r_perp_sq_efm = X_grid_efm**2 + Y_grid_efm**2\n",
        "    azimuthal_angle = torch.atan2(Y_grid_efm, X_grid_efm) # Y, X for standard angle definition\n",
        "    # Radial profile that goes to zero at boundary, e.g., Gaussian * (1 - (2r_perp/L)^2)^2\n",
        "    # For simplicity, use Gaussian for radial profile first\n",
        "    radial_profile = (A0_efm * torch.exp(-r_perp_sq_efm / (w0_efm**2)))\n",
        "    # Add z-localization to make it a vortex tube segment\n",
        "    radial_profile *= torch.exp(-Z_grid_efm**2 / (w0_efm**2)) \n",
        "    \n",
        "    phi_r_initial_cpu = (radial_profile * torch.cos(winding_n * azimuthal_angle)).to(dtype=torch.float32)\n",
        "    phi_i_initial_cpu = (radial_profile * torch.sin(winding_n * azimuthal_angle)).to(dtype=torch.float32)\n",
        "    del r_perp_sq_efm, azimuthal_angle, radial_profile\n",
        "\n",
        "phi_dot_r_initial_cpu = torch.zeros_like(phi_r_initial_cpu)\n",
        "phi_dot_i_initial_cpu = torch.zeros_like(phi_i_initial_cpu)\n",
        "\n",
        "del X_grid_efm, Y_grid_efm, Z_grid_efm; gc.collect()\n",
        "\n",
        "phi_r_curr_cpu, phi_i_curr_cpu = phi_r_initial_cpu.clone(), phi_i_initial_cpu.clone()\n",
        "phi_dot_r_curr_cpu, phi_dot_i_curr_cpu = phi_dot_r_initial_cpu.clone(), phi_dot_i_initial_cpu.clone()\n",
        "\n",
        "damping_mask_efm_sc_cpu = create_absorbing_mask_efm_sc(config_efm_sc['N'], \n",
        "                                                       config_efm_sc['boundary_width_factor'], \n",
        "                                                       config_efm_sc['damping_strength'], 'cpu')\n",
        "print(\"Complex fields initialized.\")\n",
        "\n",
        "# --- Prepare History Arrays ---\n",
        "num_history_points = config_efm_sc['T_steps'] // 100 + 1\n",
        "max_abs_phi_hist = np.zeros(num_history_points)\n",
        "energy_hist_efm_sc = np.zeros(num_history_points)\n",
        "mass_int_hist_efm_sc = np.zeros(num_history_points)\n",
        "charge_hist_efm_sc = np.zeros(num_history_points)\n",
        "Lz_hist_efm_sc = np.zeros(num_history_points)\n",
        "hist_idx_efm_sc = 0\n",
        "\n",
        "# --- Calculate Initial Observables ---\n",
        "print(\"Calculating initial observables for complex field simulation...\")\n",
        "if hist_idx_efm_sc < num_history_points:\n",
        "    abs_phi_sq_init = phi_r_curr_cpu**2 + phi_i_curr_cpu**2\n",
        "    max_abs_phi_hist[hist_idx_efm_sc] = torch.max(torch.sqrt(abs_phi_sq_init)).item()\n",
        "    del abs_phi_sq_init; gc.collect()\n",
        "    energy_hist_efm_sc[hist_idx_efm_sc] = compute_total_energy_complex_sim(phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu,\n",
        "                                                                      config_efm_sc['m_sq_param'], config_efm_sc['g_param'], config_efm_sc['eta_param'],\n",
        "                                                                      config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], config_efm_sc['c_sim']**2, device)\n",
        "    mass_int_hist_efm_sc[hist_idx_efm_sc] = compute_mass_integral_complex_sim(phi_r_curr_cpu, phi_i_curr_cpu, config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], device)\n",
        "    charge_hist_efm_sc[hist_idx_efm_sc] = compute_noether_charge_sim(phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu,\n",
        "                                                                config_efm_sc['q_sim_coupling'], config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], device)\n",
        "    Lz_hist_efm_sc[hist_idx_efm_sc] = compute_Lz_complex_sim(phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu,\n",
        "                                                       config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], config_efm_sc['L_sim'], config_efm_sc['N'], device)\n",
        "    print(f\"Initial Max|φ|: {max_abs_phi_hist[hist_idx_efm_sc]:.3f}, Energy: {energy_hist_efm_sc[hist_idx_efm_sc]:.3e}, MassInt: {mass_int_hist_efm_sc[hist_idx_efm_sc]:.3e}, Q: {charge_hist_efm_sc[hist_idx_efm_sc]:.3e}, Lz: {Lz_hist_efm_sc[hist_idx_efm_sc]:.3e}\")\n",
        "    hist_idx_efm_sc += 1\n",
        "\n",
        "# --- Simulation Time Loop ---\n",
        "print(f\"Starting complex field simulation loop for {config_efm_sc['T_steps']} steps...\")\n",
        "sim_start_time_efm_sc = time.time()\n",
        "pbar_efm_sc = tqdm(range(config_efm_sc['T_steps']), desc=\"Complex Eholokon Progress\")\n",
        "numerical_error_efm_sc = False\n",
        "\n",
        "for t_step in pbar_efm_sc:\n",
        "    try:\n",
        "        if torch.any(torch.isinf(phi_r_curr_cpu)) or torch.any(torch.isnan(phi_r_curr_cpu)) or \\\n",
        "           torch.any(torch.isinf(phi_i_curr_cpu)) or torch.any(torch.isnan(phi_i_curr_cpu)) or \\\n",
        "           torch.any(torch.isinf(phi_dot_r_curr_cpu)) or torch.any(torch.isnan(phi_dot_r_curr_cpu)) or \\\n",
        "           torch.any(torch.isinf(phi_dot_i_curr_cpu)) or torch.any(torch.isnan(phi_dot_i_curr_cpu)):\n",
        "            print(f\"\\nERROR: NaN/Inf detected BEFORE step {t_step + 1}! Stopping.\"); numerical_error_efm_sc = True; break\n",
        "        \n",
        "        phi_r_next_cpu, phi_i_next_cpu, phi_dot_r_next_cpu, phi_dot_i_next_cpu = update_phi_rk4_complex_chunked(\n",
        "            phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu, config_efm_sc['dt_sim'],\n",
        "            config_efm_sc['m_sq_param'], config_efm_sc['g_param'], config_efm_sc['eta_param'],\n",
        "            damping_mask_efm_sc_cpu, config_efm_sc['chunk_size'],\n",
        "            config_efm_sc['dx_sim'], config_efm_sc['c_sim']**2, device\n",
        "        )\n",
        "        phi_r_curr_cpu, phi_i_curr_cpu = phi_r_next_cpu, phi_i_next_cpu\n",
        "        phi_dot_r_curr_cpu, phi_dot_i_curr_cpu = phi_dot_r_next_cpu, phi_dot_i_next_cpu\n",
        "\n",
        "        if torch.any(torch.isinf(phi_r_curr_cpu)) or torch.any(torch.isnan(phi_r_curr_cpu)) or \\\n",
        "           torch.any(torch.isinf(phi_i_curr_cpu)) or torch.any(torch.isnan(phi_i_curr_cpu)):\n",
        "            print(f\"\\nERROR: NaN/Inf detected in phi AFTER step {t_step + 1}! Stopping.\"); numerical_error_efm_sc = True; break\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"ERROR at step {t_step + 1}: {e}\"); numerical_error_efm_sc = True; import traceback; traceback.print_exc(); break\n",
        "\n",
        "    if (t_step + 1) % 100 == 0:\n",
        "        if hist_idx_efm_sc < num_history_points:\n",
        "            abs_phi_sq_curr = phi_r_curr_cpu**2 + phi_i_curr_cpu**2\n",
        "            max_abs_phi_hist[hist_idx_efm_sc] = torch.max(torch.sqrt(abs_phi_sq_curr)).item()\n",
        "            del abs_phi_sq_curr; gc.collect()\n",
        "            energy_hist_efm_sc[hist_idx_efm_sc] = compute_total_energy_complex_sim(phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu, config_efm_sc['m_sq_param'], config_efm_sc['g_param'], config_efm_sc['eta_param'], config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], config_efm_sc['c_sim']**2, device)\n",
        "            mass_int_hist_efm_sc[hist_idx_efm_sc] = compute_mass_integral_complex_sim(phi_r_curr_cpu, phi_i_curr_cpu, config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], device)\n",
        "            charge_hist_efm_sc[hist_idx_efm_sc] = compute_noether_charge_sim(phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu, config_efm_sc['q_sim_coupling'], config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], device)\n",
        "            Lz_curr_val = compute_Lz_complex_sim(phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu, config_efm_sc['chunk_size'], config_efm_sc['dx_sim'], config_efm_sc['L_sim'], config_efm_sc['N'], device)\n",
        "            Lz_hist_efm_sc[hist_idx_efm_sc] = Lz_curr_val\n",
        "            \n",
        "            pbar_efm_sc.set_postfix({'Max|φ|': f'{max_abs_phi_hist[hist_idx_efm_sc]:.2f}', 'E': f'{energy_hist_efm_sc[hist_idx_efm_sc]:.2e}', 'Q':f'{charge_hist_efm_sc[hist_idx_efm_sc]:.2e}', 'Lz':f'{Lz_curr_val:.2e}'})\n",
        "            if max_abs_phi_hist[hist_idx_efm_sc] > 100 or np.isnan(energy_hist_efm_sc[hist_idx_efm_sc]):\n",
        "                print(f\"Instability. Max|φ|={max_abs_phi_hist[hist_idx_efm_sc]:.2f}. Stop.\"); numerical_error_efm_sc = True; break\n",
        "            hist_idx_efm_sc += 1\n",
        "        else: print(f\"History index out of bounds at step {t_step+1}.\")\n",
        "\n",
        "sim_end_time_efm_sc = time.time()\n",
        "print(f\"Complex field sim loop finished in {sim_end_time_efm_sc - sim_start_time_efm_sc:.2f} s. Error: {numerical_error_efm_sc}\")\n",

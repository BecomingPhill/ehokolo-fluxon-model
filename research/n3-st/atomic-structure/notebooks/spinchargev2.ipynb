        "del phi_r_curr_cpu, phi_i_curr_cpu, phi_dot_r_curr_cpu, phi_dot_i_curr_cpu, damping_mask_efm_sc_cpu\n",
        "gc.collect(); torch.cuda.empty_cache() if device.type == 'cuda' else None"
